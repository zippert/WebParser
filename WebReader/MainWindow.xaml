<Window x:Class="WebReader.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="WebParser" Height="502" Width="659">
    <Grid>
        <Label Content="URL:" Height="26" HorizontalAlignment="Left" Margin="12,12,0,0" Name="label" VerticalAlignment="Top" Width="33" />
        <TextBox Height="23" Margin="51,15,80,0" Name="urlField" VerticalAlignment="Top" />
        <Button Content="Parse" Height="23" Margin="564,15,0,0" Name="parseButton" VerticalAlignment="Top" HorizontalAlignment="Left" Width="61" Click="ParseButtonClick" />
        <Rectangle Height="400" HorizontalAlignment="Left" Margin="51,44,0,0" Name="rectangle1" Stroke="Black" VerticalAlignment="Top" Width="506" />
        <TextBlock Height="400" HorizontalAlignment="Left" Margin="51,44,0,0" Name="textBlock1" Text="Enter URL and press Parse..." FontStyle="Italic" VerticalAlignment="Top" Width="506" />
    </Grid>
</Window>
