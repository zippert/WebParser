<Window x:Class="WebReader.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="WebParser" Height="649" Width="756">
    <Grid>
        <Label Content="URL:" Height="26" HorizontalAlignment="Left" Margin="12,12,0,0" Name="label" VerticalAlignment="Top" Width="33" />
        <TextBox Text="http://" Height="23" Margin="51,15,80,0" Name="urlField" VerticalAlignment="Top" />
        <Button Content="Parse" Height="23" Margin="0,15,12,0" Name="parseButton" VerticalAlignment="Top" HorizontalAlignment="Right" Width="61" Click="ParseButtonClick" />
        <TextBox Margin="51,44,80,351" Name="textBlock1" Text="Enter URL and press Parse..." FontStyle="Italic" ScrollViewer.HorizontalScrollBarVisibility="Auto" ScrollViewer.VerticalScrollBarVisibility="Auto" ScrollViewer.CanContentScroll="True"/>
        <Label Content="Result:" Margin="12,265,0,317" Name="label1" HorizontalAlignment="Left" Width="47" />
        <ListBox Margin="51,0,80,35" Name="resultList" Height="276" VerticalAlignment="Bottom" />
    </Grid>
</Window>
